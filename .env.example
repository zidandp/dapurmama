# Database Configuration (dari Neon Dashboard)
DATABASE_URL=postgres://your_user:your_password@your_host-pooler.region.aws.neon.tech/your_database?sslmode=require
DATABASE_URL_UNPOOLED=postgresql://your_user:your_password@your_host.region.aws.neon.tech/your_database?sslmode=require

# Neon Database Parameters
PGHOST=your_host-pooler.region.aws.neon.tech
PGHOST_UNPOOLED=your_host.region.aws.neon.tech
PGUSER=your_user
PGDATABASE=your_database
PGPASSWORD=your_password

# Vercel Postgres Format (untuk deployment)
POSTGRES_URL=postgres://your_user:your_password@your_host-pooler.region.aws.neon.tech/your_database?sslmode=require
POSTGRES_URL_NON_POOLING=postgres://your_user:your_password@your_host.region.aws.neon.tech/your_database?sslmode=require
POSTGRES_USER=your_user
POSTGRES_HOST=your_host-pooler.region.aws.neon.tech
POSTGRES_PASSWORD=your_password
POSTGRES_DATABASE=your_database
POSTGRES_URL_NO_SSL=postgres://your_user:your_password@your_host-pooler.region.aws.neon.tech/your_database
POSTGRES_PRISMA_URL=postgres://your_user:your_password@your_host-pooler.region.aws.neon.tech/your_database?connect_timeout=15&sslmode=require

# Optional: Neon Auth (jika menggunakan Neon Auth)
NEXT_PUBLIC_STACK_PROJECT_ID=your_stack_project_id
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=your_publishable_key
STACK_SECRET_SERVER_KEY=your_secret_server_key